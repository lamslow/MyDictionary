<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mydictionary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.ListHistoryVNActivity"
            android:parentActivityName=".activity.ListFavoriteActivity"></activity>
        <activity
            android:name=".activity.ListFavoriteVNActivity"
            android:parentActivityName=".activity.ListFavoriteActivity" />
        <activity
            android:name=".activity.DetailVietNamWordActivity"
            android:parentActivityName=".activity.SearchVAActivity" />
        <activity
            android:name=".activity.ListHistoryActivity"
            android:parentActivityName=".activity.HomePageActivity" />
        <activity
            android:name=".activity.ListFavoriteActivity"
            android:parentActivityName=".activity.HomePageActivity" />
        <activity
            android:name=".activity.DetailWordActivity"
            android:parentActivityName=".activity.SearchAVActivity" />
        <activity
            android:name=".activity.SearchAVActivity"
            android:parentActivityName=".activity.ChoseSearchActivity" />
        <activity
            android:name=".activity.SearchVAActivity"
            android:parentActivityName=".activity.ChoseSearchActivity" />
        <activity
            android:name=".activity.ChoseSearchActivity"
            android:parentActivityName=".activity.HomePageActivity" />
        <activity android:name=".activity.HomePageActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>